#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int n;
    scanf("%d", &n);
    int r,temp;
    int sum=0;
    temp=n;
    //Complete the code to calculate the sum of the five digits on n.
    for (int i=0; i<=n; i++) {
            r=temp%10;
            sum=sum+r;
            temp=temp/10;
            //printf("%i",r);
            
            
    
    }
    printf("%d",sum);
    return 0;
}
